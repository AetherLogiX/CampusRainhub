    "configurations": [
        {
            "name": "CMake Debug (RainHub)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/RainHubd.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "${env:PATH};E:/Qt/6.5.3/mingw_64/bin;C:/Program Files/MySQL/MySQL Server 8.0/lib"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
